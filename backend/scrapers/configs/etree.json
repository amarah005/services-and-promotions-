  },
  "category_paths": {
    "default": "/",
    "shop": "/shop/",
    "plants": "/plants/"
  },
  "selectors": {
    "product_container": ".product, .woocommerce-loop-product, .product-item, .product-card",
    "title": ".product-title, .woocommerce-loop-product__title, h2, h3, .product-name",
    "price": ".price, .woocommerce-Price-amount, .amount, .product-price",
    "image": "img",
    "url": "a",
    "brand": ".brand, .product-brand, .manufacturer",
    "rating": ".rating, .star-rating, .woocommerce-product-rating",
    "review_count": ".review-count, .woocommerce-review-link",
    "next_page": ".next, .woocommerce-pagination .next, .page-numbers .next, .pagination .next, .page-numbers a, .pagination a"
  },
  "settings": {
    "page_load_delay": 10,
    "scroll_pause": 2.0,
    "timeout": 60,
    "wait_for_lazy_images": true,
    "scroll_to_bottom": true,
