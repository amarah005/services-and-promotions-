  "settings": {
    "page_load_delay": 3,
    "scroll_pause": 2,
    "timeout": 40,
    "handle_popups": true,
    "close_ads": true,
    "wait_for_lazy_images": true,
    "scroll_to_bottom": true,
    "click_load_more": false
  }
