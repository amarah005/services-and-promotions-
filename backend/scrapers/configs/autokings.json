{
  "platform_name": "AutoKings",
  "platform_display_name": "AutoKings.pk",
  "platform_type": "ecommerce",
  "platform_base_url": "https://www.autokings.pk",
  "base_url": "https://www.autokings.pk",
  "category_paths": {
    "accessories": [
      "/collections/best-sellers",
      "/collections/cruise-control",
      "/collections/steering-wheel-controls",
      "/collections/retractable-side-mirrors",
      "/collections/paddle-shifters",
      "/collections/gadgets",
      "/collections/android-panels",
      "/collections/sale",
      "/collections/car-leds",
      "/collections/steering-wheel-emblem-logo",
      "/collections/airbags-airbag-covers",
      "/collections/spiral-cable-clock-spring-chakri"
    ]
  },
  "api_endpoints": {
    "products": "/products.json",
    "collections": "/collections.json",
    "search": "/search.json"
  },
  "selectors": {
    "product_container": ".card, .product-card, .grid-product, .product-item",
    "product_title": ".card-title, .product-title, .grid-product__title, .product__title h1, .product__title h2",
    "product_price": ".price, .product-price, .grid-product__price, .price-item--sale, .price-item--regular",
    "product_image": ".card-img, .product-image img, .grid-product__image img, .product__media img",
    "product_url": ".card-link, .product-link, .grid-product__link, .product__title a",
    "product_description": ".card-text, .product-description, .grid-product__description, .product__description",
    "product_sku": ".sku, .product-sku",
    "product_stock": ".stock, .product-stock, .badge--sold-out",
    "product_rating": ".rating, .product-rating",
    "product_brand": ".brand, .product-brand",
    "product_category": ".category, .product-category",
    "product_tags": ".tags, .product-tags",
    "product_weight": ".weight, .product-weight",
    "product_dimensions": ".dimensions, .product-dimensions",
    "product_attributes": ".attributes, .product-attributes",
    "product_variants": ".variants, .product-variants, .product-form__input select",
    "product_options": ".options, .product-options",
    "add_to_cart": ".add-to-cart, .btn-cart, .product-form__submit",
    "product_reviews": ".reviews, .product-reviews",
    "product_availability": ".availability, .product-availability",
    "product_sale_badge": ".badge--sale, .price__badge-sale",
    "product_sold_out": ".badge--sold-out, .price__badge-sold-out",
    "product_quantity": ".quantity, .product-quantity, .quantity__input",
    "product_materials": ".accordion__content",
    "product_shipping": ".accordion__content",
    "product_care": ".accordion__content",
    "product_video": ".deferred-media__poster img, .deferred-media__poster-button",
    "product_media": ".product__media img, .deferred-media__poster img",
    "product_gallery": ".product__media, .product__gallery",
    "product_video_poster": ".deferred-media__poster img",
    "product_video_button": ".deferred-media__poster-button"
  },
  "pagination": {
    "next_page": ".pagination .next a, .pagination .arrow-right a",
    "page_numbers": ".pagination .page-numbers a, .pagination .page a",
    "max_pages": 30
  },
  "search": {
    "search_url": "/search",
    "search_param": "q",
    "filters": {
      "category": "product_type",
      "price_min": "price_min",
      "price_max": "price_max",
      "brand": "vendor",
      "rating": "rating",
      "stock": "availability",
      "featured": "featured",
      "sort": "sort_by"
    }
  },
  "keywords": {
    "accessories": [
      "seat cover", "car mat", "helmet", "bike cover", "steering cover",
      "mobile holder", "air freshener", "car perfume", "sun shade", "car cover",
      "dashboard camera", "gps tracker", "keychain", "tyre inflator", "alloy rims",
      "floor mats", "fog light", "roof rack", "car charger", "usb adapter",
      "bluetooth", "speaker", "amplifier", "subwoofer", "car stereo",
      "cruise control", "steering wheel controls", "retractable mirrors",
      "paddle shifters", "car gadgets", "android panels", "car leds",
      "led lights", "headlights", "taillights", "fog lights", "interior lights"
    ]
  },
  "synonyms": {
    "cover": ["protection", "shield", "guard", "case"],
    "mat": ["carpet", "rug", "flooring", "pad"],
    "helmet": ["head protection", "safety gear", "headgear"],
    "camera": ["cam", "recorder", "dashcam", "video recorder"],
    "tracker": ["gps", "locator", "monitor", "tracking device"],
    "holder": ["mount", "bracket", "stand", "mounting"],
    "freshener": ["deodorizer", "perfume", "scent", "air freshener"],
    "shade": ["visor", "sun visor", "protection", "cover"],
    "rack": ["carrier", "holder", "mount", "roof rack"],
    "charger": ["power adapter", "charging cable", "usb charger"],
    "speaker": ["audio", "sound system", "speaker system"],
    "amplifier": ["amp", "audio amplifier", "sound amplifier"],
    "subwoofer": ["sub", "bass speaker", "woofer"],
    "stereo": ["audio system", "car stereo", "music system"],
    "control": ["controller", "remote", "steering control"],
    "mirror": ["side mirror", "rear view mirror", "car mirror"],
    "light": ["led", "bulb", "lamp", "lighting"],
    "gadget": ["device", "accessory", "electronic"],
    "panel": ["display", "screen", "android panel", "lcd panel"]
  },
  "settings": {
    "delay_between_requests": 2,
    "timeout": 30,
    "max_retries": 3,
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36",
    "handle_popups": false,
    "close_ads": false,
    "scroll_to_bottom": false,
    "wait_for_dynamic_content": false,
    "use_api_first": true,
    "api_timeout": 20
  }
}
